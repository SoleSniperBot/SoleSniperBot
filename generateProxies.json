const fs = require('fs');
const proxies = [];

const user = 'GEONODE_USERNAME:type-residential'; // replace with real username
const pass = 'YOUR_PASSWORD'; // replace with real password

for (let port = 9000; port < 19000; port++) {
  proxies.push({
    formatted: `http://${user}:${pass}@proxy.geonode.io:${port}`
  });
}

fs.writeFileSync('proxies.json', JSON.stringify(proxies, null, 2));
console.log('âœ… proxies.json created with 10,000 entries.');
